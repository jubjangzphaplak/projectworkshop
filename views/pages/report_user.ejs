<% include ../partials/header %>
    <h1><center>Top 50 User</h1></center><br>
    <div class="jumbotron"><table class="table">
        <thead>
            <tr>
                <th>Name</th>
                <th>Address</th>
                <th>E-mail</th>
                <th>Total Price</th>
            </tr>
        </thead>
        <tbody id="myTable">
         <% report.forEach(function(report) 
         { %>
    
            <tr>
                <td scope="row"><% =report.name%></td>
                <td><%=report.address%></td>
                <td><%=report.email%></td>
                <td><%=report.sum%></td>
                
            </tr></div>
          <%});%>   
        </tbody>
    </table>
    <script>
        $(document).ready(function(){
          $("#myInput").on("keyup", function() {
            var value = $(this).val().toLowerCase();
            $("#myTable tr").filter(function() {
              $(this).toggle($(this).text().toLowerCase().indexOf(value) > -1)
            });
          });
        });
        </script>
        












