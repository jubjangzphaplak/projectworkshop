<% include ../partials/header %>
<br>
<h1><center>Product</h1></center><br>
<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th></th>
            <th></th>
        </tr>
    </thead>
    <tbody>
     <% products.forEach(function(product) { %>
        
        
        <tr>
            <td scope="row"><%=product.id%></td>
            <td><%=product.title%></td>
            <td><%=product.price%></td>
            <td><a class="btn btn-danger" href="/products/<%=product.id%>" role="button">Edit</a></td>
            <td><a class="btn btn-primary" href="" role="button">New</a></td>
        </tr>
      <%});%>
           
        
    </tbody>
</table>







<% include ../partials/footer %>